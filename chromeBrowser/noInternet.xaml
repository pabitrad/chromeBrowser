<Window x:Class="chromeBrowser.noInternet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="noInternet" Height="240" Width="600" AllowsTransparency="True" WindowStyle="None"
        Background="Transparent" Topmost="True" Icon="Images/cloud_norm-over.ico">
    <Grid>
        <Canvas Margin="0,0,0,0" Width="600" Height="240">
            <Canvas.Background>
                <ImageBrush ImageSource="Images/register_key/internet_bg.png">
                </ImageBrush>
            </Canvas.Background>

            <Image  Width="26" Height="27" Canvas.Right="13" Canvas.Top="7"  
                        Source="Images/login/close.png" 
                   MouseLeftButtonDown="minClose_MouseLeftButtonDown"  Cursor="Hand" Name="minClose">
            </Image>
            <Image  Width="150" Height="39" Canvas.Left="228" Canvas.Top="178"  
                        Source="Images/register_key/back_log.png" 
                   MouseLeftButtonDown="minClose_MouseLeftButtonDown"  Cursor="Hand" Name="back">
            </Image>
            
        </Canvas>
    </Grid>
</Window>
